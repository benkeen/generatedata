{
	"presets": [
		[
			"@babel/preset-env",
			{
				"targets": {
					"esmodules": true
				}
			}
		],
		[
			"@babel/preset-react", {}
		]
	],
	"plugins": [
		"@babel/plugin-syntax-dynamic-import",
        "@babel/plugin-proposal-class-properties"
	],
	"env": {
		"BUILD": {
			"comments": true
		},
		"DEV": {
			"comments": true
		},
		"DIST": {
			"comments": true
		}
	}
}
