{
  "name": "@generatedata/i18n",
  "description": "Generatedata.com internationalization locale files, including both the core and plugins.",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "run-s clean:dist clean:lib build-src build-dist",
    "build-src": "tsc -p tsconfig.lib.json",
    "build-dist": "node ./lib/i18n.js",
    "clean": "npm-run-all clean:*",
    "clean:dist": "npx rimraf dist",
    "clean:lib": "npx rimraf lib",
    "purge": "npx rimraf .turbo node_modules dist lib"
  },
  "exports": {
    ".": "./dist/localeFileMap.ts"
  },
  "devDependencies": {
    "@generatedata/i18n-core": "workspace:*",
    "@generatedata/config": "workspace:*",
    "@generatedata/plugins": "workspace:*",
    "npm-run-all": "catalog:",
    "rimraf": "catalog:",
    "typescript": "catalog:"
  }
}
