{
  "name": "@generatedata/plugins",
  "version": "1.0.0",
  "description": "generatedata.com plugins",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "tsc": "tsc",
    "build": "run-s tsc copy-i18n-files generate-web-workers",
    "generate-web-workers": "node ./build/webworkers.build.js",
    "watch": "tsc --watch",
    "clean": "npx rimraf dist",
    "purge": "npx rimraf node_modules dist .turbo",
    "copy-i18n-files": "copyfiles -u 1 ./src/*/*/i18n/*.json dist"
  },
  "dependencies": {
    "@generatedata/core": "workspace:*",
    "@mui/material": "^7.3.5",
    "@mui/icons-material": "^7.3.5",
    "@mui/lab": "^7.0.1-beta.19",
    "date-fns": "^2.17.0",
    "randomcolor": "^0.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "reselect": "^5.1.1"
  },
  "devDependencies": {
    "@generatedata/types": "workspace:*",
    "@generatedata/config": "workspace:*",
    "@generatedata/core": "workspace:*",
    "@generatedata/i18n": "workspace:*",
    "@generatedata/utils": "workspace:*",
    "@griffel/react": "^1.5.32",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/dom": "^10.4.1",
    "@types/jest": "^30.0.0",
    "@types/randomcolor": "^0.5.9",
    "@types/react": "^18.3.27",
    "@types/sinon": "^21.0.0",
    "concurrently": "^9.2.1",
    "copyfiles": "^2.4.1",
    "jest": "^30.2.0",
    "md5-file": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "rollup": "^2.31.0",
    "rollup-plugin-strip-exports": "^2.0.6",
    "sinon": "^21.0.0",
    "typescript": "^5.9.3"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./names": {
      "types": "./dist/names.d.ts",
      "default": "./dist/names.js"
    },
    "./workerPluginsFileMap": {
      "default": "./dist/workers/workerPluginsFileMap.js"
    },
    "./dataTypes/*": {
      "default": "./dist/dataTypes/*"
    },
    "./exportTypes/*": {
      "default": "./dist/exportTypes/*"
    }
  }
}
