{
  "name": "@generatedata/i18n",
  "description": "Generatedata.com internationalization locale files, including both the core and plugins.",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "run-s build-src build-dist",
    "build-src": "tsc -p tsconfig.lib.json",
    "build-dist": "node ./lib/i18n.js",
    "purge": "npx rimraf .turbo",
    "clean": "npx rimraf dist lib node_modules .turbo"
  },
  "exports": {
    ".": "./dist/localeFileMap.js"
  },
  "devDependencies": {
    "@generatedata/i18n-core": "workspace:*",
    "@generatedata/config": "workspace:*",
    "@generatedata/plugins": "workspace:*",
    "crypto-browserify": "catalog:",
    "npm-run-all": "catalog:",
    "rimraf": "catalog:",
    "typescript": "catalog:"
  }
}
