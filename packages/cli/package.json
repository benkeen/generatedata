{
  "name": "@generatedata/cli",
  "version": "0.0.3",
  "description": "The CLI version of generatedata.com. Currently in development only.",
  "license": "GPL3",
  "main": "dist/cli/index.js",
  "bin": {
    "generatedata": "./bin/generatedata.js"
  },
  "scripts": {
    "__build": "grunt && tsc",
    "__clean": "npx rimraf dist",
    "__purge": "npx rimraf node_modules dist .turbo",
    "test": "jest"
  },
  "files": [
    "dist/*",
    "bin/generatedata.js"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "\\.(ts|tsx)$": "ts-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/../../client/tests/jestSetup.ts"
    ],
    "testRegex": "/__tests__/.*\\.(ts|tsx)$",
    "moduleNameMapper": {
      "\\.(css|less|scss|sss|styl)$": "<rootDir>/../../node_modules/jest-css-modules",
      "^~components(.*)$": "../../client/src/components$1",
      "^~types(.*)$": "<rootDir>/../../client/types$1",
      "^~utils(.*)$": "<rootDir>/../../client/src/utils$1",
      "^~store(.*)$": "<rootDir>/../../client/src/core/store$1",
      "^~core(.*)$": "<rootDir>/../../client/src/core$1"
    }
  },
  "devDependencies": {
    "@generatedata/plugins": "workspace:*",
    "@types/jest": "catalog:",
    "grunt": "catalog:",
    "grunt-cli": "catalog:",
    "jest": "catalog:",
    "jest-cli": "catalog:",
    "jest-css-modules": "catalog:",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "ansi-colors": "catalog:",
    "cli-progress": "catalog:",
    "yargs": "catalog:"
  }
}
