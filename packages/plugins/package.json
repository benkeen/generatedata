{
  "name": "@generatedata/plugins",
  "version": "1.0.0",
  "description": "generatedata.com plugins",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "tsc": "tsc",
    "build": "run-s validateI18nFiles tsc copy-i18n-files generate-web-workers",
    "copy-i18n-files": "copyfiles -u 1 ./src/*/*/i18n/*.json dist",
    "dev": "npm run watch",
    "generate-web-workers": "node ./config/webworkers.build.js",
    "watch": "tsc --watch",
    "clean": "npx rimraf dist",
    "purge": "npx rimraf node_modules dist .turbo",
    "test": "jest --config=./config/jest.config.js",
    "validateI18nFiles": "node ./scripts/validate-plugin-i18n.js",
    "sortI18nFiles": "node ./scripts/sort-plugin-i18n.js"
  },
  "dependencies": {
    "@generatedata/shared": "workspace:*",
    "@mui/material": "catalog:",
    "@mui/icons-material": "catalog:",
    "@mui/lab": "catalog:",
    "date-fns": "catalog:",
    "randomcolor": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "reselect": "catalog:"
  },
  "devDependencies": {
    "@babel/runtime": "catalog:",
    "@generatedata/config": "workspace:*",
    "@generatedata/shared": "workspace:*",
    "@generatedata/tools": "workspace:*",
    "@generatedata/utils": "workspace:*",
    "@griffel/react": "catalog:",
    "@react-hook/throttle": "catalog:",
    "@rollup/plugin-babel": "catalog:",
    "@rollup/plugin-commonjs": "catalog:",
    "@rollup/plugin-node-resolve": "catalog:",
    "@rollup/plugin-terser": "catalog:",
    "@testing-library/react": "catalog:",
    "@testing-library/dom": "catalog:",
    "@types/jest": "catalog:",
    "@types/randomcolor": "catalog:",
    "@types/react": "catalog:",
    "@types/sinon": "^21.0.0",
    "concurrently": "^9.2.1",
    "copyfiles": "catalog:",
    "jest": "catalog:",
    "md5-file": "catalog:",
    "npm-run-all": "catalog:",
    "rimraf": "catalog:",
    "rollup": "catalog:",
    "rollup-plugin-strip-exports": "catalog:",
    "sinon": "catalog:",
    "typescript": "catalog:",
    "ts-jest": "catalog:"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./names": {
      "types": "./dist/names.d.ts",
      "default": "./dist/names.js"
    },
    "./workerPluginsFileMap": {
      "default": "./dist/workers/workerPluginsFileMap.js"
    },
    "./dataTypes": {
      "default": "./dist/dataTypes"
    },
    "./dataTypes/*": {
      "default": "./dist/dataTypes/*"
    },
    "./exportTypes": {
      "default": "./dist/exportTypes"
    },
    "./exportTypes/*": {
      "default": "./dist/exportTypes/*"
    },
    "./countries": {
      "default": "./dist/countries"
    },
    "./countries/*": {
      "default": "./dist/countries/*"
    }
  }
}
