{
  "name": "@generatedata/client",
  "description": "Client-side code for generatedata app",
  "private": true,
  "scripts": {
    "build": "run-s build-assets create-generation-worker-bundle build-webpack",
    "build-assets": "tsx ./config/build.ts",
    "build-webpack": "npm run webpackProd",
    "bundle-generation-worker": "node ./config/generationWorker.build.js",
    "clean": "tsx ./config/build.ts clean",
    "dev": "npm run watch",
    "worker-tsc": "tsc -p ./tsconfig.workers.json",
    "create-generation-worker-bundle": "run-s worker-tsc bundle-generation-worker",
    "coverage": "npx jest --coverage",
    "purge": "npx rimraf node_modules dist .turbo",
    "watch": "npm run build-assets & npm run build & npm run webpackDev",
    "test": "jest --config=./config/jest.config.js",
    "webpackDev": "webpack serve --config ./webpack.config.js --mode=development",
    "webpackProd": "webpack --config ./webpack.config.js --mode=production"
  },
  "globals": {
    "ts-jest": {
      "tsconfig": {
        "allowJs": true
      }
    }
  },
